@using Microsoft.AspNetCore.Identity;
@inject SignInManager<User> signInManager;

@{
    ViewData["Title"] = "RankUp";
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Meta Tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="Create an account or log into RankUp. Build your profile, add your CV, request a review, and our HR team will assist you career advancement tips."> 
    <meta name="keywords" content="Career Cv Review Courses Jobs Internship Intern Interview Linkedin Business Startup Guidance Freshgraduate Fresh grduates Students Case study Event Development Advancement Workshop Session">
    <!-- Page Title -->
    <title>RankUp</title>
    <!-- Favicon -->
    <link href="~/Content/corporate-agency/images/favicon.ico" rel="icon">
    <!-- Bundle -->
    <link href="~/Content/vendor/css/bundle.min.css" rel="stylesheet">
    <!-- Plugin Css -->
    <link href="~/Content/vendor/css/LineIcons.min.css" rel="stylesheet">
    <link href="~/Content/vendor/css/revolution-settings.min.css" rel="stylesheet">
    <link href="~/Content/vendor/css/jquery.fancybox.min.css" rel="stylesheet">
    <link href="~/Content/vendor/css/owl.carousel.min.css" rel="stylesheet">
    <link href="~/Content/vendor/css/cubeportfolio.min.css" rel="stylesheet">
    <!-- Style Sheet -->
    <link href="~/Content/corporate-agency/css/navigation.css" rel="stylesheet">
