    <div class="loader-bg">
        <div class="loader"></div>
    </div>
    <!-- Loader ends -->
    <!-- START HEADER -->
    <header>
        <!--Navigation-->
        <nav class="navbar navbar-top-default navbar-expand-lg navbar-simple nav-line">
            <div class="container">
                <a href="index-corporate-agency.html" title="Logo" class="logo scroll">
                    <!--Logo Default-->
                    <img src="~/Content/corporate-agency/images/logo.png" alt="logo" class="ml-lg-3 m-0">
                </a>

                <!--Nav Links-->
                <div class="collapse navbar-collapse" id="Shuvo">
                    <div class="navbar-nav ml-auto">
                        <a class="nav-link scroll line" href="#slider-section">Home</a>
                        <a class="nav-link scroll line" href="#about">About Us</a>
                        <a class="nav-link scroll line" href="#individuals">For Individuals</a>
                        <a class="nav-link scroll line" href="#business">For Business</a>
                        <!-- <a class="nav-link scroll line" href="#contact">Contact Us</a> -->
                        @if (signInManager.IsSignedIn(User))
                        {
                            <span style="color: #1DB954;"> @User.Identity.Name</span>
                            <a class="nav-link scroll line" href="" onclick="document.location='Account/LogOut'">Log Out</a>
                        }
                        else
                        {
                            <a href="account/register" class="btn btn-large btn-rounded btn-pink nav-button" @*data-animation-duration="500" data-fancybox data-src="#RigesterModal"*@>Join Us</a>
                            <span style="color: #1DB954;"> OR</span>
                            <a href="Account/LogIn" class="nav-link line">Log In</a>
                        }
                    </div>
                </div>
            </div>

